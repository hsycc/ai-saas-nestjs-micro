{
  "name": "ai-saas-nestjs-micro",
  "version": "0.0.1",
  "description": "",
  "author": "hsycc0316@gmail.com",
  "private": true,
  "license": "",
  "husky": {
    "hooks": {}
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "clear": "rimraf ./dist/*",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "compodoc": "compodoc -p tsconfig.json -s",
    "docs:proto-gen": "bash ./generate-proto-docs.sh",
    "docker": "docker-compose up -d",
    "docker:build": "docker-compose build",
    "docker:teardown": "docker-compose down -v",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:traefik": "docker-compose -f docker-compose.traefik.yml up -d",
    "docker:seed:user": "docker exec -it user-svc pnpm run seed:user",
    "docker:seed:ai": "docker exec -it ai-svc pnpm run seed:ai",
    "seed": "pnpm run seed:user && pnpm run seed:ai",
    "seed:user": "ts-node prisma/scripts/seed-user.ts",
    "seed:ai": "ts-node prisma/scripts/seed-ai.ts",
    "prisma:generate": "pnpm run prisma:generate:user && pnpm run prisma:generate:ai",
    "prisma:generate:user": "prisma generate --schema=prisma/user.prisma",
    "prisma:generate:ai": "prisma generate --schema=prisma/ai.prisma",
    "prisma:migrate": "pnpm run prisma:migrate:user && pnpm run prisma:migrate:ai",
    "prisma:migrate:user": "prisma migrate dev -n user  --schema=prisma/user.prisma",
    "prisma:migrate:ai": "prisma migrate dev -n ai  --schema=prisma/ai.prisma",
    "prisma:deploy": "pnpm run prisma:deploy:user && pnpm run prisma:deploy:ai",
    "prisma:deploy:user": "prisma migrate deploy--schema=prisma/user.prisma",
    "prisma:deploy:ai": "prisma migrate deploy --schema=prisma/ai.prisma",
    "prisma:push": "pnpm run prisma:push:user && pnpm run prisma:push:ai",
    "prisma:push:user": "prisma db push --schema=prisma/user.prisma",
    "prisma:push:ai": "prisma db push --schema=prisma/ai.prisma"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@grpc/grpc-js": "^1.8.14",
    "@metinseylan/nestjs-opentelemetry": "^3.0.0",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/cli": "^9.4.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/microservices": "^9.4.0",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/terminus": "^9.2.2",
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/exporter-zipkin": "^1.13.0",
    "@opentelemetry/sdk-trace-base": "^1.13.0",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/passport": "^1.0.12",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "fluent-ffmpeg": "^2.1.2",
    "helmet": "^6.1.5",
    "ip": "^1.1.8",
    "multer": "1.4.5-lts.1",
    "nacos": "^2.5.1",
    "nest-winston": "^1.9.1",
    "nestjs-cls": "^3.4.0",
    "nestjs-prisma": "^0.20.0",
    "openai": "^3.2.1",
    "passport": "^0.6.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "passport-strategy": "^1.0.0",
    "prisma": "^4.15.0",
    "protobufjs": "^7.2.3",
    "rxjs": "^7.2.0",
    "ts-loader": "^9.4.3",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.6.3",
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "commander": "^10.0.1",
    "commitizen": "^4.3.0",
    "commitlint": "^16.3.0",
    "concurrently": "^8.0.1",
    "conventional-changelog": "^3.1.25",
    "conventional-changelog-cli": "^2.2.2",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "google-protobuf": "^3.21.2",
    "husky": "^8.0.3",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "rimraf": "^5.0.0",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-node": "^10.0.0",
    "ts-proto": "^1.146.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "astTransformers": {
      "before": []
    },
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/_proto/",
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/proto(|/.*)$": "<rootDir>/_proto/$1",
      "^@lib/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@lib/config(|/.*)$": "<rootDir>/libs/config/src/$1",
      "^@lib/grpc(|/.*)$": "<rootDir>/libs/grpc/src/$1",
      "^@lib/logger(|/.*)$": "<rootDir>/libs/logger/src/$1",
      "^@lib/swagger(|/.*)$": "<rootDir>/libs/swagger/src/$1",
      "^@lib/open-ai(|/.*)$": "<rootDir>/libs/open-ai/src/$1",
      "^lib/nacos(|/.*)$": "<rootDir>/libs/nacos/src/$1"
    }
  }
}
